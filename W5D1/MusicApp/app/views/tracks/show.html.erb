<ul>
  <li><%= @track.album.title %></li>
  <li><%= @track.title %></li>
  <li><%= @track.ord %></li>
  <li>
    <% if @track.bonus %>
      Bonus
    <% else %>
      Regular
    <% end %>
  </li>
  <p>Lyrics: <%= @track.lyrics %></p>
</ul>


<%= button_to "Edit Track", edit_track_url(@track), method: :get %>
<%= button_to "Back to Album", album_url(@track.album), method: :get %>
